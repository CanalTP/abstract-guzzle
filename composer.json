{
    "name": "canaltp/abstract-guzzle",
    "license": "MIT",
    "type": "library",
    "description": "Allows to use multiple versions of Guzzle in a same project.",
    "homepage": "https://github.com/CanalTP/abstract-guzzle",
    "require": {
        "php": ">=5.4",
        "guzzlehttp/psr7": "~1.3",
        "guzzlehttp/guzzle": "~3.0|~5.0|~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.3",
        "squizlabs/php_codesniffer": "2.x",
        "guzzlehttp/guzzle": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "CanalTP\\AbstractGuzzle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CanalTP\\AbstractGuzzle\\Tests\\": "test/"
        }
    }
}
